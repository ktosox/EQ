[sub_resource type="Shader" id=13]
code = "shader_type canvas_item;	
render_mode blend_mix;	
	
uniform float range = 1.0;	
	
void fragment() {	
	float goal = sin((range/0.02)*(UV.y));
	goal = goal * cos((range/0.02)*(UV.x));
	vec4 col = texture(TEXTURE, UV);
	col.a = col.a * goal;
	COLOR = col;
}	"

